{
  "name": "server",
  "version": "1.0.0",
  "description": "Server of palawatta warriers cricket club management application",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon dist/index.js",
    "ts:check": "tsc",
    "user:create": "node ./dist/commands/createUser.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "test:unit": "npx jest --testPathPattern=dist/tests/unit"
  },
  "author": "sandeepa097",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.0.3",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "express-validator": "^7.0.1",
    "file-type": "^16.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.3.0",
    "nodemon": "^3.1.0",
    "os": "^0.1.2",
    "sequelize": "^6.37.1"
  }
}
