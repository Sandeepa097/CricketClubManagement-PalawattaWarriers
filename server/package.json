{
  "name": "server",
  "version": "1.0.0",
  "description": "Server of palawatta warriers cricket club management application",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon index.ts",
    "migrate": "npx sequelize-cli db:migrate --config dist/config/database.js",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --config dist/config/database.js",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all --config dist/config/database.js"
  },
  "author": "sandeepa097",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "http-status-codes": "^2.3.0",
    "mariadb": "^3.3.0",
    "nodemon": "^3.1.0",
    "sequelize": "^6.37.1"
  }
}
